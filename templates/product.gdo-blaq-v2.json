    "section_TAQrRD": {
      "type": "section",
      "blocks": {
        "text_xGcgef": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<h3>FREQUENTLY ASKED QUESTIONS</h3>",
            "width": "100%",
            "max_width": "normal",
            "alignment": "center",
            "type_preset": "rte",
            "font": "var(--font-body--family)",
            "font_size": "1rem",
            "enable_mobile_size": false,
            "mobile_font_size": "1rem",
            "line_height": "normal",
            "letter_spacing": "normal",
            "case": "none",
            "wrap": "pretty",
            "color": "var(--color-foreground)",
            "background": false,
            "background_color": "#00000026",
            "corner_radius": 0,
            "padding-block-start": 0,
            "padding-block-end": 36,
            "padding-inline-start": 0,
            "padding-inline-end": 0,
            "custom_mobile_padding": false,
            "mobile-padding-block-start": 0,
            "mobile-padding-block-end": 0,
